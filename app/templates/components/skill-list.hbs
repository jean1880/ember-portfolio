<div class="skill-list-component col-md-4">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3>{{title}}</h3>
    </div>
    <div class="panel-body">
      <ul>
        {{#each skills as |skill|}}
          <li>{{skill}}</li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
{{yield}}
